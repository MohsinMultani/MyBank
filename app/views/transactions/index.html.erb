<p id="notice"><%= notice %></p>

<h1>Transactions</h1>

<table>
  <thead>
    <tr>
      <th>account_id</th>
      <th>transaction_type</th>
      <th>transaction_amount</th>
    </tr>
  </thead>

  <tbody>
    <% @transactions.each do |transaction| %>
      <tr>
        <td><%= transaction.account_id %></td>
        <td><%= transaction.transaction_type %></td>
        <td><%= transaction.transaction_amount %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
